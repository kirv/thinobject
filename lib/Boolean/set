#!/bin/bash

synopsis="Boolean.set"

if test -n "$boolean_states"; then
    states=(${boolean_states/:/ })
else
    states=(true false)
fi

ln -sf "=${states[0]}" $TOB_object

exit
    
NAME
    Boolean.set -- set boolean attribute to true value

SYNOPSIS
    Boolean.set [STATE]

AUTHOR
    ken.irving@alaska.edu (c) 2011

