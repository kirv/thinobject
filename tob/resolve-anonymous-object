resolve_anonymous_object () { # try to resolve object type at path
    if [[ -L $path/^ ]]; then
        type=($(readlink $path/^))
    else
        type=Directory
    fi

    cd $path ||
        error -x 2 "failed: cd $path/"
    anonymous=1
    object=$path
    method="${q[*]:m}"
    method=${method// /.}
}
