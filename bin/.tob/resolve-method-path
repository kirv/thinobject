resolve_method_path () {
    test "$1" = --super && super=1 && shift || unset super
    local method=$1
    for path in ${mpaths[@]}; do
        test -x $path/$method && {
            test -n "$super" && unset super && continue
            TOB_method_path=$path/$method
            return
            }
    done
    return 1
    }


