resolve_chained_methods () {
    local -a methods=(${TOB_method//./ })
    test ${#methods[*]} -gt 1 || return 2
    declare -a order
    declare -A chain
    for m in ${methods[*]}; do
        resolve_method_path $m || return 1
        chain+=( [$m]=$TOB_method_path )
        order+=( $m )
    done
    }
